package oops_p;

class AAA{
	int a = 10;
	int b = 20;
	int sa = 1111;
	
	public AAA() {
		System.out.println("AAA 기본생성자");
	}

	int setA() {
		System.out.println("setA()실행");
		return 50;
	}
	@Override
	public String toString() {
		return "AAA [a=" + a + ", b=" + b + ", sa=" + sa + "]";
	}
}

public class InstanceMain {

	public static void main(String[] args) {
		AAA a1 = new AAA();
		System.out.println(a1);
	}

}
